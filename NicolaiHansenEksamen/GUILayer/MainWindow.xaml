<Window x:Class="NicolaiHansenEksamen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NicolaiHansenEksamen"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="ChckNameTbx" HorizontalAlignment="Left" Margin="81,217,0,0" Text="Check Summoner name" TextWrapping="Wrap" VerticalAlignment="Top" Width="141" GotFocus="ChckNameTbx_GotFocus"/>
        <Button x:Name="ChckNameBtn" Content="Check" HorizontalAlignment="Left" Margin="132,240,0,0" VerticalAlignment="Top" Click="ChckNameBtn_Click"/>
        <Label x:Name="ChckNameResultLbl" Content="" HorizontalAlignment="Left" Margin="130,284,0,0" VerticalAlignment="Top"/>
        <Button x:Name="SpillereBtn" Content="Spillere" HorizontalAlignment="Center" Margin="0,138,0,0" VerticalAlignment="Top" Height="39" Width="54" Click="SpillereBtn_Click"/>

    </Grid>
</Window>
